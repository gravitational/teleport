version: v2
storage:
  type: directory
  path: /bot/storage
  symlinks: secure
  acls: try
outputs:
  - type: identity
    destination:
      type: directory
      path: /bot/output
    roles:
      - editor
    cluster: example.teleport.sh
  - type: identity
    destination:
      type: memory
  - type: identity
    destination:
      type: kubernetes_secret
      name: my-secret
services:
  - type: spiffe-workload-api
    listen: unix:///var/run/spiffe.sock
    svids:
      - path: /bar
        hint: my hint
        sans:
          dns:
            - foo.bar
          ip:
            - 10.0.0.1
        rules:
          - unix:
              pid: 100
              uid: 1000
              gid: 1234
          - unix:
              pid: 100
  - type: example
    message: llama
debug: true
auth_server: example.teleport.sh:443
certificate_ttl: 1m0s
renewal_interval: 30s
oneshot: true
fips: true
diag_addr: 127.0.0.1:1337
