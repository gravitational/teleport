should request certificates when certManager enabled:
  1: |
    apiVersion: cert-manager.io/v1
    kind: Certificate
    metadata:
      labels:
        app.kubernetes.io/instance: RELEASE-NAME
        app.kubernetes.io/managed-by: Helm
        app.kubernetes.io/name: teleport-plugin-event-handler
        app.kubernetes.io/version: 19.0.0-dev.updater.8
        helm.sh/chart: teleport-plugin-event-handler-19.0.0-dev.updater.8
      name: RELEASE-NAME-teleport-plugin-event-handler-server
      namespace: NAMESPACE
    spec:
      dnsNames:
      - test.example.com
      - test2.example.com
      duration: 87600h
      issuerRef:
        group: cert-manager.io
        kind: Issuer
        name: RELEASE-NAME-teleport-plugin-event-handler-ca-issuer
      privateKey:
        algorithm: ECDSA
        encoding: PKCS8
        size: 256
      secretName: RELEASE-NAME-teleport-plugin-event-handler-server-tls
      usages:
      - server auth
  2: |
    apiVersion: cert-manager.io/v1
