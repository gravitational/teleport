# tbot config file generated by `configure` command
version: v2
onboarding:
  join_method: token
storage:
  type: directory
  path: /var/lib/teleport/bot
  symlinks: secure
  acls: try
debug: false
proxy_server: proxy.example.com:443
credential_ttl: 1h0m0s
renewal_interval: 20m0s
oneshot: false
fips: false
