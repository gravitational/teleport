{
  "extends": "../../../../tsconfig.base.json",
  "compilerOptions": {
    "allowUnreachableCode": false,
    "composite": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "outDir": "../../../../build.assets/.cache/ts/design-new",
    "paths": {
      "shared-new/*": ["web/packages/new/shared/src/*"],
      "shared-new": ["web/packages/new/shared/src/"],
    },
    "pretty": true,
    "rootDir": "../../../..",
    "strict": true,
    "verbatimModuleSyntax": true,
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "../shared",
    "../../../../gen/proto/ts/**/*.ts",
  ],
}