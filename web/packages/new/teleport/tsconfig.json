{
  "extends": "../../../../tsconfig.base.json",
  "compilerOptions": {
    "allowUnreachableCode": false,
    "composite": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "outDir": "../../../../build.assets/.cache/ts/teleport-new",
    "paths": {
      "design-new/*": ["web/packages/new/design/src/*"],
      "design-new": ["web/packages/new/design/src/"],

      "shared-new/*": ["web/packages/new/shared/src/*"],
      "shared-new": ["web/packages/new/shared/src/"],

//      "teleport-new/*": ["web/packages/new/teleport/src/*"],
//      "teleport-new": ["web/packages/new/teleport/src/"],

      "gen-proto-ts/*": ["gen/proto/ts/*"],
    },
    "pretty": true,
    "rootDir": "../../../..",
    "strict": true,
    "verbatimModuleSyntax": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "../../../@types",
    "../",
    "../../../../gen/proto/ts",
  ],
}