{
  "extends": "../../../../tsconfig.base.json",
  "compilerOptions": {
    "allowUnreachableCode": false,
    "composite": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "outDir": "../../../../build.assets/.cache/ts/shared-new",
    "paths": {
      "teleport-new/*": ["web/packages/new/teleport/src/*"],
      "teleport-new": ["web/packages/new/teleport/src/"]
    },
    "pretty": true,
    "rootDir": "../../../..",
    "strict": true,
    "verbatimModuleSyntax": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "../../../../gen/proto/ts/**/*.ts",
  ],
}